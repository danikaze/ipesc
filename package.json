{
  "name": "ipesc",
  "description": "Utilities and data for the IPESC racing community.",
  "version": "1.0.0",
  "scripts": {
    "dev": "webpack serve",
    "clean": "rimraf dist && rimraf lib/dist",
    "build": "rimraf dist && webpack --mode=production --node-env=production",
    "test": "npm run lint && npm run test:unit && npm run test:typecheck",
    "test:unit": "jest",
    "test:unit:dev": "node --inspect node_modules/jest/bin/jest.js --watch --runInBand",
    "test:typecheck": "tsc --noEmit",
    "lint": "eslint",
    "prettier": "prettier --write '**/*.{ts,tsx,md,html,css,less}'",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danikaze/ipesc.git"
  },
  "author": "danikaze <danikaze@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danikaze/ipesc/issues"
  },
  "homepage": "https://github.com/danikaze/ipesc#readme",
  "devDependencies": {
    "@alex_neo/jest-expect-message": "^1.0.5",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/webpack": "^5.28.3",
    "@typescript-eslint/parser": "^6.7.5",
    "@webpack-cli/generators": "^3.0.7",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "sass": "^1.69.3",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
